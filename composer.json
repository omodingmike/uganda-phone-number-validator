{
    "name": "omodingmike/uganda-phone-number-validator",
    "description": "This is my package uganda-phone-number-validator",
    "keywords": [
        "omodingmike",
        "uganda-phone-number-validator"
    ],
    "homepage": "https://github.com/omodingmike/uganda-phone-number-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "omodingmike",
            "email": "omodingmike@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "OmodingMike\\PhoneNumberValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OmodingMike\\PhoneNumberValidator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
